        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/jfrog-logo-200.png"
    },
    "scopes": [
        "vso.build",
        "vso.build_execute"
    ],
    "categories": [
        "Build and release"
    ],
    "tags": [
        "JFrog",
        "Artifactory",
        "Distribution",
        "Xray",
        "Artifact",
        "Build",
        "Release",
        "Promote",
        "Upload",
        "Download",
