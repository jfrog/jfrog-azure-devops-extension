        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://www.jfrog.com/confluence/display/JFROG/JFrog+Artifactory"
        },
        "learn": {
            "uri": "https://www.jfrog.com/confluence/display/JFROG/Artifactory+Azure+DevOps+Extension"
